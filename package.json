{
  "name": "ringtail-deploy-web",
  "version": "0.13.0",
  "description": "Website for interacting with the Ringtail deployment service",
  "author": "Brian Mancini <bmancini@gmail.com>",
  "license": "MIT",
  "engine": "node >= 0.6.2",
  "scripts": {
    "start": "babel-node src/server/server.js",
    "watch": "nodemon  src/server/server",
    "test": "npm run lint && npm run test:server",
    "test:file": "babel-node node_modules/.bin/_mocha",
    "lint": "eslint .",
    "test:client": "karma start karma.conf.js",
    "test:server": "babel-node node_modules/.bin/_mocha --recursive test/server",
    "test:watch": "node_modules/.bin/_mocha -w --recursive test/server --compilers js:babel-register"
  },
  "main": "src/server/server.js",
  "repository": "fti-technology/ringtail-deploy-web",
  "dependencies": {
    "activedirectory": "^0.7.2",
    "async": "^2.0.0",
    "body-parser": "^1.15.0",
    "chalk": "^1.1.1",
    "cheerio": "^0.20.0",
    "cookie-parser": "^1.4.3",
    "debug": "^2.2.0",
    "edge": "git://github.com/adampaulberg/edge",
    "eslint": "^2.1.0",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "hops-model": "^0.3.0",
    "hops-sqlite": "^0.1.0",
    "migrate": "^0.2.2",
    "morgan": "^1.7.0",
    "nib": "^1.1.0",
    "node-skytap": "^0.7.0",
    "q": "^1.4.1",
    "request": "^2.69.0",
    "ringtail-deploy-client": "git://github.com/fti-technology/ringtail-deploy-client",
    "serve-static": "^1.10.2",
    "sqlite3": "^3.1.1",
    "statements": "^0.1.0",
    "stylus": "^0.54.5",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "bower": "^1.7.7",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "jshint": "^2.9.1",
    "karma": "^0.13.21",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^1.1.5",
    "karma-ng-html2js-preprocessor": "^0.2.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.1.0",
    "mocha": "^2.4.5",
    "nock": "^7.2.1",
    "nodemon": "^1.8.1",
    "phantomjs-prebuilt": "^2.1.4",
    "sinon": "^1.17.3"
  },
  "babel": {
    "plugins": [
      "transform-async-to-generator"
    ]
  }
}
